import java.util.Scanner;

public class java9 {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);

        int marks;

        System.out.print("Enter your mark: ");
        marks = input.nextInt();

        if (marks >= 40){
            System.out.println("You passed");
        }else {
            System.out.println("You failed");
        }

        input.close();
    }
}
